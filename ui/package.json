  "scripts": {
    "babelrc": "cross-env node .babelrc.js",
    "clean:client": "cross-env rimraf dist && mkdir  dist",
    "clean:server": "cross-env rimraf server-dist && mkdir  server-dist",
    "webpack:init": "cross-env npm-run-all clean:* babelrc",
    "webpack:client": "cross-env NODE_ENV=production webpack --config webpack.client.config.js --progress --colors",
    "webpack:server": "cross-env NODE_ENV=production webpack --config webpack.server.config.js --progress --colors",
    "build": "cross-env npm-run-all webpack:init webpack:client webpack:server",
    "start": "cross-env NODE_ENV=production node server-dist/server.js --colors",
    "webpack:client:dev": "cross-env NODE_ENV=development webpack --config webpack.client.config.js --colors --debug --output-pathinfo --devtool inline-source-map",
    "webpack:server:dev": "cross-env NODE_ENV=development webpack --config webpack.server.config.js --colors --debug --output-pathinfo --devtool inline-source-map",
    "webpack:dev": "cross-env npm-run-all webpack:init webpack:*:dev",
    "webpack:watch": "cross-env npm-run-all --parallel 'webpack:*:dev -- --watch'",
    "start:dev": "cross-env APP_ENV=development NODE_ENV=development nodemon --watch dist --watch server-dist --ext js server-dist/server.js",
    "development": "cross-env yarn run webpack:dev && yarn run nf -p 3000 start",
    "dev": "cross-env yarn run development",
    "test": "cross-env npm-run-all lint unit security",
    "test:sonarqube": "cross-env  npm-run-all test sonarqube",
    "lint": "cross-env eslint 'src' --config .eslintrc --ext .js,.jsx",
    "lint:fix": "cross-env yarn run lint --fix",
    "unit": "cross-env APP_ENV=development BABEL_ENV=test jest",
    "unit:clear": "yarn run unit -u",
    "unit:watch": "yarn run unit --watchAll",
    "security": "nsp check --preprocessor yarn",
    "preanalyze": "cross-env NODE_ENV=production webpack --config webpack.client.config.js --profile --json > .analyze.json",
    "analyze": "webpack-bundle-analyzer -s parsed .analyze.json",
    "open:cover": "yarn run test && open coverage/lcov-report/index.html",
    "sonarqube": "sonarqube-scanner-node -Dsonar.login=${SONARQUBE_TOKEN} -Dsonar.projectVersion=${VERSION:-local}",
    "commit": "git-cz"
  }
